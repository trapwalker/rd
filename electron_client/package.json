{
  "name": "RoadDogs",
  "version": "1.0.0",
  "description": "Road Dogs Online Game Client",
  "main": "main.js",
  "scripts": {
    "start": "electron . --devtools",
    "start-steam": "electron . --steam --devtools",
    "build-win-64": "electron-packager .  RoadDogs --overwrite --platform=win32 --arch=x64 --out=build",
    "build-ubuntu": "electron-packager .  RoadDogs --overwrite --platform=linux --arch=x64 --prune=true --out=build",
    "build-mac": "electron-packager .  RoadDogs --overwrite --platform=darwin --arch=x64 --prune=true --out=build",
    "rebuild_greenworks": "electron-rebuild -f -w greenworks"
  },
  "dependencies": {
    "electron-config": "^1.0.0",
    "greenworks": "git+https://github.com/greenheartgames/greenworks.git",
    "jquery": "^3.2.1"
  },
  "devDependencies": {
    "electron": "^1.7.6",
    "electron-config": "^1.0.0",
    "electron-rebuild": "^1.6.0"
  }
}
